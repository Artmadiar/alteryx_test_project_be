  "name": "backend-project",
  "version": "1.0.0",
  "description": "Test project: REST API",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "rm -rf ./build && tsc",
    "prod": "npm run build && pm2 start build/index.js",
    "eslint": "eslint src",
    "db:create": "./node_modules/.bin/sequelize db:create",
    "db:drop": "./node_modules/.bin/sequelize db:drop",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:migration:new": "./node_modules/.bin/sequelize migration:generate --name",
    "docs": "./node_modules/.bin/apidoc -i src/lib -i src/router -o build/public/docs/",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/jest --detectOpenHandles --forceExit"
  },
